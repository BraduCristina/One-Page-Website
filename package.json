{
  "name": "one-page-website",
  "version": "1.0.0",
  "description": "My web-site",
  "main": "index.js",
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^3.3.7",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "jquery": "^3.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-flow-react-proptypes": "^3.4.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "empty": "^0.10.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "flow-bin": "^0.49.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "nodemon": "^1.11.0",
    "pm2": "^2.5.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  },
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack --progress",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow && jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "repository": "git+https://github.com/BraduCristina/One-Page-Website.git",
  "author": "KMovie",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BraduCristina/One-Page-Website/issues"
  },
  "homepage": "https://github.com/BraduCristina/One-Page-Website#readme"
}
